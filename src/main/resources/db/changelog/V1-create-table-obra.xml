<?xml version="1.0" encoding="UTF-8"?>   
<databaseChangeLog
   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:pro="http://www.liquibase.org/xml/ns/pro"
   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
      http://www.liquibase.org/xml/ns/pro 
      http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">

    <!-- O id deve ser Ãºnico -->
   <changeSet  author="felipe.favato"  id="0">
      <createTable tableName="obra">
        <!-- ID -->
        <column  name="codigo"  autoIncrement="true" type="int">
          <constraints  nullable="false"  primaryKey="true"  unique="true"/>
        </column>

        <!-- NOME -->
        <column  name="nome"  type="VARCHAR(100)">
          <constraints  nullable="false"/>
        </column>

        <!-- ENDERECO -->
        <column  name="endereco"  type="VARCHAR(100)">
          <constraints  nullable="true" />
        </column>

        <!-- DATA INICIO -->
        <column  name="data_inicio"  type="DATE">
          <constraints  nullable="true" />
        </column>

        <!-- DATA PREVISTA FIM -->
        <column  name="data_prevista_fim"  type="DATE">
          <constraints  nullable="true" />
        </column>

        <!-- DATA FIM -->
        <column  name="data_real_fim"  type="DATE">
          <constraints  nullable="true" />
        </column>

        <!-- CUSTO PREVISTO -->
        <column  name="custo_previsto"  type="int">
          <constraints  nullable="true" />
        </column>

      </createTable>
   </changeSet>

   <changeSet id="1" author="felipe.favato">
    <createSequence sequenceName="sq_obra" incrementBy="1" startValue="1"/>
   </changeSet>

   <!-- <changeSet id="2" author="felipe.favato">
    <addColumn tableName="obra">
      <column name="compras" type="array" />
    </addColumn>
   </changeSet> -->

</databaseChangeLog>